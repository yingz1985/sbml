{
    i = 0;
    while(i<100)
    {

        if (i mod 3 == 0 andalso i mod 5 == 0)
        {
            print('fizzbuzz');
        }
        else
        {
            if(i mod 3 == 0)
            {
                print('fizz');
            }
            else
            {

                if (i mod 5 == 0)
                {
                    print("buzz");
                }
                else
                {
                    print('-');
                }
            }
        }

        i = i+1;
    }
}