{
    row=15;
    col=18;
    result_str=""  ;
    i = 1;
    while (i<row+1)
    {
        result_str="";
        if((i<=3)orelse(i>=7 andalso i<=9)orelse(i>=13 andalso i<=15))
        {

            j = 1;
            while(j < col)
            {
                result_str=result_str+"o";
                j = j +1;
            }
            print(result_str);
        }
        else
        {
            if(i>=4 andalso i<=6)
            {
                j = 1;
                while(j<5)
                {
                    result_str=result_str+"o";
                    j = j+1;
                }
                print(result_str);
            }
            else
            {

                j = 1;
                while(j<14)
                {
                    result_str=result_str+" ";
                    j = j+1;
                }
                j = 1;
                while(j<5)
                {
                    result_str=result_str+"o";
                    j = j +1;
                }

                print(result_str);
            }

        }

        i = i + 1;
     }

    print(result_str);
}